  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.FileProviders.Embedded" Version="9.0.5" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Localization\Ordering\*.json" />
    <Content Remove="Localization\Ordering\*.json" />
  </ItemGroup>
</Project>