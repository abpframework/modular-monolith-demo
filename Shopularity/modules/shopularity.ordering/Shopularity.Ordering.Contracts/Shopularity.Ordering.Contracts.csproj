    <ProjectReference Include="../../shopularity.catalog/Shopularity.Catalog.Contracts/Shopularity.Catalog.Contracts.csproj" />
    <ProjectReference Include="../../shopularity.payment/Shopularity.Payment.Contracts/Shopularity.Payment.Contracts.csproj" />
